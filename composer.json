{
    "name": "fortis/moneyobject",
    "description": "A PHP library providing immutable Money value object",
    "keywords": ["money", "currency"],
    "homepage": "https://github.com/fortis/moneyobject",
    "license": "MIT",
    "authors": [
        {
            "name": "Alan Bondarchuk",
            "email": "imacoda@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "brick/math": "^0.5.4",
        "fortis/iso-currency": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "5.*",
        "satooshi/php-coveralls": "1.*"
    },
    "autoload": {
        "psr-4": {
            "Money\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Money\\Tests\\": "tests/"
        }
    }
}
